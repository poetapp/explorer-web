{
  "name": "explorer-web",
  "version": "1.0.1",
  "description": "Po.et's web application to view claims on the Po.et Network",
  "scripts": {
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "tslint -p ./tsconfig.json --fix",
    "start": "node devServer.js",
    "build:development": "POET_ENV=development webpack",
    "build:testing": "POET_ENV=testing webpack",
    "build:preview": "stylelint './src/**/*.scss' && tslint -p ./tsconfig.json && npm run build:testing",
    "build:production": "POET_ENV=production webpack",
    "build": "webpack",
    "serve": "ws --spa index.html -d ./dist",
    "stylelint": "stylelint './src/**/*.scss' ",
    "stylelint:fix": "stylelint './src/**/*.scss' --fix",
    "test": "npm run test:unit",
    "test:unit": "ts-node --files -r ignore-styles -r tsconfig-paths/register tests/unit/index.ts",
    "test:e2e": "testcafe 'chrome:headless,firefox:headless' tests/functional/features",
    "coverage": "nyc --report-dir ./.coverage npm run test:unit",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poetapp/explorer-web.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poetapp/explorer-web/issues"
  },
  "greenkeeper": {
    "ignore": [
      "webpack",
      "webpack-dev-middleware",
      "webpack-dev-server",
      "webpack-hot-middleware",
      "awesome-typescript-loader",
      "html-webpack-plugin",
      "babel-loader"
    ]
  },
  "dependencies": {
